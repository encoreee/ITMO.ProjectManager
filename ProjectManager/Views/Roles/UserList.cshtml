@model IEnumerable<ProjectManager.User>


    <div class="intro">
        <div class="intro__inner">
            <h1 class="header-content-title"> Admin area > List of users </h1>
            <div class="admin-area">
                <table class="table">
                    @foreach (var user in Model)
                    {
                        <tr>
                            <td>@user.Email</td>
                            <td>
                                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Acsess level</a>
                            </td>
                        </tr>
                    }
                </table>
            </div>
        </div>
    </div>